{
  "name": "@antv/g2plot",
  "version": "0.1.0-beta.6",
  "description": "G2 Plot, a market of plots built with the Grammar of Graphics'",
  "author": "https://github.com/orgs/antvis/people",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "esm",
    "dist",
    "README.md",
    "LICENSE"
  ],
  "homepage": "https://antvis.github.io/g2plot",
  "repository": {
    "type": "git",
    "url": "https://github.com/antvis/g2plot"
  },
  "scripts": {
    "start": "npm run site:develop",
    "site:develop": "gatsby develop --open -H 0.0.0.0",
    "site:build": "npm run site:clean && gatsby build --prefix-paths",
    "site:clean": "gatsby clean",
    "site:deploy": "npm run site:build && gh-pages -d public",
    "prepublishOnly": "run-s lint-check test build",
    "publish": "npm publish && tnpm sync",
    "build": "run-s clean lib dist",
    "clean": "rimraf lib esm dist",
    "lib": "run-p lib:*",
    "lib:cjs": "tsc -p tsconfig.json --target ES5 --module commonjs --outDir lib",
    "lib:esm": "tsc -p tsconfig.json --target ES5 --module ESNext --outDir esm",
    "dist": "webpack --config webpack.config.js --mode production",
    "lint": "lint-staged",
    "lint-check": "eslint --ext .ts --quiet ./src ./__tests__ ./demos",
    "lint-fix": "run-s lint-fix:*",
    "lint-fix:examples": "prettier --write examples/**/*.js",
    "lint-fix:eslint": "eslint --ext .ts --fix ./src ./__tests__ ./demos",
    "test": "jest",
    "test-live": "DEBUG_MODE=1 jest --watch",
    "coverage": "jest --coverage",
    "compare-live": "node scripts/compare.js",
    "ci": "run-s lint-check build test coverage",
    "demos-web": "webpack-dev-server --config webpack-dev.config.js --port 2046 --openPage demos/index.html"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s lint test build "
    }
  },
  "dependencies": {
    "@antv/component": "~0.4.0-beta.4",
    "@antv/coord": "~0.2.2",
    "@antv/dom-util": "~2.0.1",
    "@antv/event-emitter": "^0.1.1",
    "@antv/g": "~3.5.0-beta.1",
    "@antv/g-gesture": "~0.1.0-beta.2",
    "@antv/g2": "~3.6.0-beta.5",
    "@antv/gui": "~0.1.0-beta.4",
    "@antv/matrix-util": "~2.0.2",
    "@antv/scale": "^0.2.1",
    "@antv/util": "~2.0.4",
    "fecha": "~3.0.3",
    "resize-observer-polyfill": "^1.5.1",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@antv/gatsby-theme-antv": "^0.9.77",
    "@types/jest": "^24.0.23",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "colors": "^1.4.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "event-simulate": "^1.0.0",
    "gatsby": "^2.16.0",
    "gh-pages": "^2.1.1",
    "husky": "^3.0.4",
    "jest": "^24.9.0",
    "jest-electron": "^0.1.7",
    "jest-extended": "^0.11.2",
    "lint-staged": "^9.2.3",
    "npm-run-all": "^4.1.5",
    "nunjucks": "~3.2.0",
    "prettier": "^1.18.2",
    "puppeteer": "^2.0.0",
    "react-i18next": "^11.0.1",
    "resemblejs": "^3.2.3",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.5.3",
    "webpack": "^4.39.2",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.9.0"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "diagnostics": {
          "warnOnly": true
        }
      }
    },
    "runner": "jest-electron/runner",
    "testEnvironment": "jest-electron/environment",
    "setupFilesAfterEnv": [
      "jest-extended"
    ],
    "preset": "ts-jest",
    "collectCoverage": false,
    "collectCoverageFrom": [
      "src/**/*.{ts,js}",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "testRegex": "/__tests__/.*-spec\\.ts?$"
  },
  "lint-staged": {
    "*.{js,css,json}": [
      "prettier --write",
      "git add"
    ],
    "*.ts": [
      "eslint --ext .ts --fix ./src ./__tests__ ./demos",
      "git add"
    ]
  },
  "keywords": [
    "g2",
    "plot"
  ],
  "license": "MIT"
}
